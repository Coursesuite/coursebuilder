<section data-action-source="media/media/:id" data-source-init="UI.Selection.Start">
{{#each .}}
	<figure data-name="{{name}}" data-type="media" data-extn="{{type}}" data-changed="{{changed}}" data-folder="{{path}}" data-action="select">
		<p><i class="material-icons">video_label</i></p>
		<p>{{name}}</p>
		<figcaption><i class="material-icons">check</i></figcaption>
	</figure>
{{/each}}
</section>