<configuration>
    <system.webServer>
        <handlers accessPolicy="Read, Execute, Script" />
        <security>
            <requestFiltering>
                <hiddenSegments>
                    <remove segment="bin" />
                </hiddenSegments>
            </requestFiltering>
        </security>
    </system.webServer>
</configuration>
